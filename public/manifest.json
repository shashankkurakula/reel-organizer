{
    "name": "Instagram Reel Organizer",
    "short_name": "Reel Organizer",
    "description": "Save & Organize your Instagram Reels easily!",
    "start_url": "/",
    "scope": "/",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#ffffff",
    "protocol_handlers": [
      {
        "protocol": "web+reelorganizer",
        "url": "/add-reel?url=%s"
      }
    ],
    "icons": [
      {
        "src": "/icons/icon-192x192.png",
        "sizes": "192x192",
        "type": "image/png"
      },
      {
        "src": "/icons/icon-512x512.png",
        "sizes": "512x512",
        "type": "image/png"
      }
    ]
  }
  
